    "test": "vitest",
    "test:api-single": "pnpm -C packages/api test:--single",
    "test:api-multiple": "pnpm -C packages/api test:--multiple",
    "test:axios": "pnpm -C axios packages/test",
    "test:hook": "pnpm -C hook test"
  },
