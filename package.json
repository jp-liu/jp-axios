{
  "name": "@jp-liu/monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@6.32.3",
  "homepage": "https://github.com/jp-liu/jp-axios#readme",
  "bugs": {
    "url": "https://github.com/jp-liu/jp-axios/issues"
  },
  "license": "MIT",
  "author": "jp-liu <fortune_coder@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:jp-liu/jp-axios.git"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "pnpm -C examples dev && pnpm build",
    "build": "rimraf packages/*/dist && pnpm -r --filter !examples run build",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "lint": "eslint .",
    "test": "vitest",
    "test:api-single": "pnpm -C packages/api test:--single",
    "test:api-multiple": "pnpm -C packages/api test:--multiple",
    "test:axios": "pnpm -C axios packages/test",
    "test:hook": "pnpm -C hook test"
  },
  "jp-api": {
    "input": [
      {
        "dirName": "test1",
        "path": "./packages/api/src/test/swagger.json"
      },
      {
        "dirName": "test2",
        "path": "./packages/api/src/test/swagger.json"
      }
    ],
    "output": "./examples/test"
  },
  "dependencies": {
    "@jp-liu/api": "workspace:^0.0.1",
    "@jp-liu/axios": "workspace:^0.0.1",
    "@jp-liu/hook": "workspace:^0.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.5",
    "@types/node": "^17.0.21",
    "bumpp": "^7.1.1",
    "eslint": "^8.10.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "vitest": "^0.7.7"
  }
}
