{
  "compilerOptions": {
    "target": "es2017",
    "module": "CommonJS",
    "lib": ["esnext", "DOM"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "types": ["axios", "node"]
  }
}
